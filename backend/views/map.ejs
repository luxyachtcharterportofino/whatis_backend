      <!-- MAPPA -->
      <!-- Filtri POI -->
      <div id="poiFilterControls" style="position: absolute; top: 70px; left: 260px; z-index: 1000; display: flex; gap: 10px; background: rgba(0, 0, 0, 0.85); padding: 10px 15px; border-radius: 8px; box-shadow: 0 2px 8px rgba(0, 0, 0, 0.5); border: 1px solid rgba(255, 255, 255, 0.1);">
        <button id="filterAllPOIs" class="poi-filter-btn active" onclick="setPOIFilter('all')" style="padding: 8px 16px; border: 2px solid #00bfff; background: linear-gradient(135deg, #00bfff, #0099cc); color: white; border-radius: 6px; cursor: pointer; font-weight: 600; transition: all 0.3s; white-space: nowrap;">
          üìç Tutti i POI
        </button>
        <button id="filterDefinitivePOIs" class="poi-filter-btn" onclick="setPOIFilter('definitive')" style="padding: 8px 16px; border: 2px solid #28a745; background: linear-gradient(135deg, #28a745, #20c997); color: white; border-radius: 6px; cursor: pointer; font-weight: 600; transition: all 0.3s; white-space: nowrap;">
          ‚úÖ POI Definitivi
        </button>
        <button id="filterProvisionalPOIs" class="poi-filter-btn" onclick="setPOIFilter('provisional')" style="padding: 8px 16px; border: 2px solid #ffc107; background: linear-gradient(135deg, #ffc107, #e0a800); color: #000; border-radius: 6px; cursor: pointer; font-weight: 600; transition: all 0.3s; white-space: nowrap;">
